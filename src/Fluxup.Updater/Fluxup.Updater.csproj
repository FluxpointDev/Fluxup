<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Fluxup</PackageId>
    <Authors>Fluxpoint</Authors>
    <Company>Fluxpoint</Company>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/FluxpointDev/Fluxup</RepositoryUrl>
    <Version>0.0.0.1</Version>
    <Description>Welcome to the world of cross platform Updating!!</Description>
    <Copyright>MIT</Copyright>
    <PackageReleaseNotes>* First Release, only can do Shortcuts and CheckForUpdate Almost works</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="5.2.7" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3-beta1" />
  </ItemGroup>

</Project>
